<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="endGamePane" prefHeight="324.0" prefWidth="510.0" stylesheets="@../CSS/dialogPages.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="it.polimi.ingsw.client.view.GUI.scenes.EndGameScene">
    <ImageView fitHeight="65.0" layoutX="145.0" layoutY="15.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/publisherMaterial/TitleSmall.png"/>
    </ImageView>

    <ImageView fx:id="winnerToken" fitWidth="70.0" layoutX="30.0" layoutY="160.0" pickOnBounds="true"
               preserveRatio="true">
        <Image url="@../images/misc/firstPlayerToken.png"/>
    </ImageView>

    <ImageView fitWidth="70.0" layoutX="400.0" layoutY="165.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/publisherMaterial/Publisher.png"/>
    </ImageView>

    <Button fx:id="closeButton" layoutX="465.0" layoutY="15.0" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0"
            text="X"/>

    <AnchorPane fx:id="rankingPane" layoutX="125.0" layoutY="150.0" prefHeight="160.0" prefWidth="250.0">
        <GridPane prefHeight="160.0" prefWidth="250.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="78.0" minWidth="10.0" prefWidth="50.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="154.0" minWidth="10.0" prefWidth="142.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="90.0" minWidth="10.0" prefWidth="60.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
    </AnchorPane>
    <Label fx:id="endMessage" alignment="CENTER" layoutX="145.0" layoutY="80.0" maxHeight="-Infinity"
           maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="210.0"
           text="Final ranking:" textAlignment="CENTER"/>
    <Label fx:id="victoryMessage" alignment="CENTER" layoutX="145.0" layoutY="110.0" maxHeight="-Infinity"
           maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="30.0" prefWidth="210.0"
           textAlignment="CENTER"/>
</AnchorPane>
